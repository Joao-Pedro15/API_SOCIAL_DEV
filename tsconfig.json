{
"compilerOptions": {
    "resolveJsonModule": true,
    "incremental":true,
    "outDir": "dist",
    "rootDirs": ["src"],
    "target": "ES2021",
    "sourceMap": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "baseUrl": "./src",
    "paths": {
    "@/*": ["*"]
    },
    "strict": false,
    "noImplicitOverride": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true
}, "include": ["src"]
}